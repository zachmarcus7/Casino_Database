<!DOCTYPE html>
<html>
 <head>
  <title>Update Dealer Qualifications</title>
 </head>
 <body>
 <div class="main_header">
    <div><header><a href="/" id="head">Gambler Tracker Database</a></header></div>
    <div class="menu">
      <button>Menu</button>
      <div class="menu_links">
          <a href="/">Index</a>
          <a href="/Dealers">Dealers</a>
              <a href="/Tables">Tables</a>
              <a href="/Games">Games</a>
              <a href="/Game_types">Game Types</a>
              <a href="/Supervisors">Supervisors</a>
              <a href="/Qualifications">Qualifications</a>
              <a href="/Game_search">Search Games</a>
              <a href="/edit_game_types">Edit Game Type</a>
              <a href="/Dealer_Search">Search Dealers</a>
              <a href="/Edit_Qualifications">Edit Qualifications</a>
              <a href="/Edit_Dealers">Edit Dealers</a>
              <a href="/Edit_Supervisors">Edit Supervisors</a>
      </div>
    </div>
   </div>
    <div class="table_container">
    <h2>Update:</h2>
    <form method="GET">
      {{#each dealer_qual}}
      <label for="dealer">Dealer: {{dealer_fname}} {{dealer_lname}}</label>
      <select id="qualification">
        <option selected="selected">{{qualification_name}}</option>
      {{/each}}
        {{#each qual}}
        <option value="{{qualification_ID}}">{{qualification_name}}</option>
        {{/each}}
      </select><br>
      {{#each dealer_qual}}
      <button type="button" id="updateButton" value="{{dealer_ID}}" class="buttons">Update</button>
      <button type="button" id="cancelButton" value="{{qualification_ID}}" class="buttons">Cancel</button>
      {{/each}}
    </form>
    </div>

 </body>
</html>
